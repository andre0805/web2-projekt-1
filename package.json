{
  "name": "projekt-1",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build": "npm run clean && npx tsc && npx prisma generate && npm run copy-files && echo \"Build complete!\" && npm run files",
    "clean": "rm -rf dist",
    "copy-files": "mkdir -p dist/ && cp -r src/views/ dist/views/",
    "dev": "open -u http://localhost:3000 && nodemon src/server.ts",
    "start": "node dist/server.js",
    "files": "find . | sed -e \"s/[^-][^\\/]*\\//  |/g\" -e \"s/|\\([^ ]\\)/|-\\1/\""
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-openid-connect": "^2.17.0",
    "path": "^0.12.7",
    "pg-promise": "^11.5.4",
    "prisma": "^5.4.2",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  }
}
